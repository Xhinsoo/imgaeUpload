<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Image upload</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>
<body>
    <h1 class="mb-3">Image Upload </h1>
    <form action="/home" method = "POST" enctype="multipart/form-data">
        <div class="mb-3">
          <label for="imageUrl" class="form-label" >Image url</label>
          <input type="text" class="form-control mb-3" id="url" name="image[url]">

          <input type="file"  name="avatar" class="mb-3"> <br>
          
          <label for="imageTitle" class="form-label" >Image Title</label>
          <input type="text" class="form-control" id="title" name="image[title]">

        </div>
     
        <button type="submit" class="btn btn-primary">Submit</button>
      </form>

<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>

</body>
</html>